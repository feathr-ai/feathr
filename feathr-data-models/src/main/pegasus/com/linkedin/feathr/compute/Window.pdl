namespace com.linkedin.feathr.compute

/**
 * Represents a time window used in sliding window algorithms.
 */
record Window {
  /**
   * Represents the duration of the window.
   */
  size: int

  /**
   * Represents a unit of time.
   */
  unit: enum Unit {
    /** A day. */
    DAY
    /** An hour. */
    HOUR
    /** A minute. */
    MINUTE
    /** A second. */
    SECOND
  }
}
